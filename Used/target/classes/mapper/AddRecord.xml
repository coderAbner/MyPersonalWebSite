<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.used.apple.dao.AddRecord">
	<insert id="addRecord" useGeneratedKeys="true">
		INSERT INTO iphone(model,time,color,area,network,surface,price) VALUES(#{model},#{time},#{color},#{area},#{network},#{surface},#{price})
	</insert>
</mapper>